                        <input type="text" value="{{ cartItem.quantity }}" onfocus="showUpdate('{{cartItem.variant_id}}')" onkeypress='return productQuantity(event);' data-zs-quantity data-zs-product-variant-id="{{cartItem.variant_id}}">
                        <div class="theme-cart-quantity-error" data-quantity-error-cart="{{cartItem.variant_id}}" style="display:none">{{i18n.get("product.message.error.invalid_quantity")}}</div>
                        <button class="zpbutton zpbutton-type-primary zpbutton-size-sm theme-cart-qty-change" data-theme-update="{{cartItem.variant_id}}" data-zs-update data-zs-product-variant-id="{{cartItem.variant_id}}">{{i18n.get("cart.update.label")}}</button>
                        <div class="zpbutton zpbutton-type-primary zpbutton-size-sm theme-cart-update-loading-button">
                          <svg class="theme-cart-button-loading-container" viewBox="0 0 24 24" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
