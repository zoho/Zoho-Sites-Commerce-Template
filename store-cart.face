{% extends "_theme" %}

{% block header_block %}
{% endblock %}

{% block cart_checkout_remove_block %}
{% endblock %}

{% block cart_checkout_block %}

{% if page.template_vars.header_style == "06" %}

<div data-theme-header-six-res="theme-header-six-res" data-headercontainer="zptheme-data-headercontainer" class="zpheader-style-01 theme-cart-checkout-headersix {% if page.template_vars.is_mobile_header_fixed %}theme-mobile-header-fixed{% endif %} {% if page.template_vars.is_site_full_stretch %}theme-pages-full-stretch{% endif %}">

  {% if page.is_contact_information_enabled || page.is_social_icon_enabled && !page.template_vars.is_social_icon_floater_enabled || page.is_portal_enabled %}

  <div class="theme-header-topbar {% if !page.is_portal_enabled %}theme-headersix-hide-topbar{% endif %} {% if page.template_vars.topbar_background == "dark" %}zpdark-header-portion{% endif %}">
    <div class="zpcontainer">
        {% if page.is_contact_information_enabled %}
        <div data-topbarinfo-position="zptheme-topbarinfo-position">
            {% include '_contacts' %}
        </div>
        {% endif %}

        {% if page.is_social_icon_enabled && !page.template_vars.is_social_icon_floater_enabled %}

        		{% include '_social-links' %}

        {% endif %}

        {% if page.is_portal_enabled %}

        <div class="theme-portal-login">
            <div class="theme-portal-container" data-portal-loggedout style="display:block">
                <div class="theme-portal-content"> <a data-portal-signin href="javascript:;">{{ i18n.get("portal.signin") }}</a> </div>
                {% if page.is_portal_signup_enabled %}
                <div class="theme-portal-content"> <a data-portal-signup href="javascript:;">{{ i18n.get("portal.signup") }}</a> </div>
                {% endif %}
            </div>
            <div class="theme-portal-container" data-portal-loggedin style="display:none">
                <div class="theme-portal-content {% if page.is_store_enabled %}theme-portal-username-content{% endif %}">
                    <a class="{% if page.is_store_enabled %}theme-user-name-dropdown{% endif %}" data-portal-profile href="javascript:;">
                        <span data-portal-user-name></span>
                        {% if page.is_store_enabled %}
                        <span class="theme-user-profile-down-arrow"></span>
                        {% endif %}
                    </a>
                    {% if page.is_store_enabled %}
                    <ul class="theme-username-dropdown-list">
                        <li>
                            <a href="/account/profile">{{i18n.get("portal.profile.title")}}</a>
                        </li>
                        <li>
                            <a href="/account/orders">{{i18n.get("orders.title.myorders")}}</a>
                        </li>
                        <li>
                            <a href="/account/address">{{i18n.get("address.tab.title")}}</a>
                        </li>
                    </ul>
                    {% endif %}
                </div>
                <div class="theme-portal-content">
                    <a data-portal-logout href="javascript:;">{{ i18n.get("portal.signout") }}</a>
                </div>
            </div>
        </div>


        {% endif %}

    </div>
  </div>

  {% endif %}

  <div class="theme-header {% if page.template_vars.header_background == "dark" %}zpdark-header-portion{% endif %}">
      <div class="zpcontainer">

          {% include '_branding' %}

          {% if page.is_search_enabled || page.is_store_enabled %}
          <div class="theme-navigation-and-icons">
             <div class="theme-search-cart-non-res theme-header-06-search-cart">
                <div class="theme-search-and-mini-cart-group">
                  {% if page.is_search_enabled %}
                   <div class="theme-search theme-search-and-mini-cart-group-item">
                      <div class="theme-search-field-container" data-search="zptheme-search-container">
                         <form method="get" action="/search-products"><input type="text" name="q" placeholder="Search" autofocus="" data-search-input="zptheme-search-input"><input type="submit" name="" class="theme-search-go" value="Go" data-zs-search=""></form>
                      </div>
                      <div class="theme-search-hide-overlay" data-theme-search-overlay="theme-search-overlay" style="display:none;"></div>
                      <span class="theme-search-icon" onclick="toggleSearch()">
                         <svg viewBox="0 9 17 18" xmlns="http://www.w3.org/2000/svg">
                            <path d="M6.11998304,9 C9.49467136,9 12.2401638,11.7792946 12.2401638,15.1954945 C12.2401638,16.6310021 11.7542814,17.9528473 10.9418418,19.0044723 L16.7453489,24.8898164 C17.0883131,25.2377224 17.0843596,25.7977325 16.7364536,26.1406967 C16.5642797,26.3106962 16.3399198,26.3953006 16.11556,26.3953006 C15.8872467,26.3953006 15.6589333,26.3075334 15.485771,26.131999 L9.6721825,20.2361782 C8.6697783,20.9620366 7.44380495,21.3911866 6.11998304,21.3911866 C2.74529472,21.3911866 0,18.611892 0,15.1954945 C0,11.7792946 2.74529472,9 6.11998304,9 Z M6.11998304,19.8186909 C8.62787144,19.8186909 10.6678658,17.7446967 10.6678658,15.1954945 C10.6678658,12.6464899 8.62787144,10.5724956 6.11998304,10.5724956 C3.61248999,10.5724956 1.57229797,12.6464899 1.57229797,15.1954945 C1.57229797,17.7446967 3.61248999,19.8186909 6.11998304,19.8186909 Z M4.50063869,16.7758971 C4.64592899,16.6284323 4.85052144,16.5440256 5.05669529,16.5440256 C5.26306681,16.5440256 5.46548485,16.6284323 5.61294956,16.7758971 C5.75843753,16.921385 5.84284427,17.1238031 5.84284427,17.3301746 C5.84284427,17.5385228 5.75843753,17.7409409 5.61294956,17.8864289 C5.46548485,18.0338936 5.26306681,18.1163236 5.05669529,18.1163236 C4.85052144,18.1163236 4.64592899,18.0338936 4.50063869,17.8864289 C4.35515072,17.7409409 4.2705463,17.5385228 4.2705463,17.3301746 C4.2705463,17.1238031 4.35515072,16.921385 4.50063869,16.7758971 Z M3.05228224,15.2674477 C2.77810858,13.2116395 4.82640523,12.2782235 4.91377708,12.2394794 C5.25970635,12.0864798 5.66236803,12.2416538 5.81773969,12.5865947 C5.97311135,12.9315356 5.81912341,13.3381508 5.47517087,13.4956968 C5.42614775,13.518034 4.27865093,14.0561023 4.4158366,15.0855878 C4.46604576,15.4623542 4.20155812,15.8082835 3.8249894,15.8584926 C3.79415228,15.8626438 3.76351283,15.8646205 3.73326872,15.8646205 C3.39366501,15.8646205 3.09834025,15.6131793 3.05228224,15.2674477 Z" stroke="none" fill-rule="evenodd"></path>
                         </svg>
                      </span>
                   </div>
                   {% endif %}
                   {% if page.is_store_enabled && page.show_cart_icon %}
                   <div class="theme-mini-cart theme-search-and-mini-cart-group-item" data-zs-view-cart>
                      <span class="theme-minicart-icon">
                         <svg viewBox="0 0 24 21" xmlns="http://www.w3.org/2000/svg">
                            <g transform="translate(0 -.036)" fill="transparent" fill-rule="evenodd">
                               <path d="M2.818 2.676H23.31l-3.015 12.058H5.835L2.817 2.676z"></path>
                            </g>
                            <ellipse cx="7.445" cy="18.323" rx="2.001" ry="2" stroke="none"></ellipse>
                            <ellipse cx="2" cy="1.999" rx="2" ry="1.999" stroke="none"></ellipse>
                            <ellipse cx="17.242" cy="18.322" rx="2" ry="1.999" stroke="none"></ellipse>
                         </svg>
                         <span class="theme-mini-cart-count" data-zs-view-cart-count>8</span>
                      </span>
                   </div>
                   {% endif %}
                </div>
             </div>
          </div>
          {% endif %}
      </div>
  </div>

</div>
{% endif %}

{% endblock %}

{% block content_area %}
<div class="theme-content-area {% if page.template_vars.is_site_full_stretch %}theme-pages-full-stretch{% endif %}" data-theme-content-container="theme-content-container">
  <div class="theme-content-container">
    <div class="theme-content-area-inner">
      <div class="zpcontainer">
        <div class="zprow">
          <div class="zpcol-md-12">
            <div class="theme-cart-area-container">
              <div class="theme-shopping-cart-table-container">
                {% if cart.items | size != 0 %}
                <div class="theme-cart-table-row theme-cart-head" data-cart-table>
                {% else %}
                <div class="theme-cart-table-row theme-cart-empty" data-cart-table>
                {% endif %}
                  <div class="theme-cart-table-data theme-cart-items">{{i18n.get("cart.items.label")}}</div>
                  <div class="theme-cart-table-data theme-cart-qty">{{i18n.get("cart.quantity.label")}}</div>
                  <div class="theme-cart-table-data theme-cart-price-label">{{i18n.get("cart.sub_total.label")}}</div>
                  <div class="theme-cart-table-data theme-cart-controle">{{i18n.get("cart.remove.label")}}</div>
                </div>

                {% for cartItem in cart.items %}

                <div class="theme-cart-table-row" data-cart-item data-zs-product-id="{{cartItem.variant_id}}">
                  <div class="theme-cart-table-data theme-cart-items">
                    <div class="theme-cart-item-img">
                      <a href="{{cartItem.url}}">
                        {% if cartItem.images[0].is_placeholder_image %}
                          <img alt="{{cartItem.images[0].alternate_text}}" title="{{cartItem.images[0].title}}" src="{{cartItem.images[0].url}}">
                        {% else %}
                          <img alt="{{cartItem.images[0].alternate_text}}" title="{{cartItem.images[0].title}}" src="{{cartItem.images[0].url}}/200x200">
                        {% endif %}
                      </a>
                    </div>
                    <div class="theme-cart-item-info">
                      <div><a href="{{cartItem.url}}">{{cartItem.name}}</a></div>
                      <ul>
                        {% for option in cartItem.options %}
                          <li>{{ option.name }}:
                            {% if option.color_code != "" %}
                            <span class="theme-cart-color" style="background-color:{{option.color_code}};"></span>
                            {% endif %}
                            <span> {{option.value}} </span>
                          </li>
                        {% endfor %}
                        {% for field in cartItem.custom_fields %}
                          {% if field.value != "" %}
                              <li>{{ field.label }}:
                                  <span> {{ field.display_value }} </span>
                              </li>
                          {% endif %}
                        {% endfor %}
                      </ul>
                      <div data-zs-cartitem-price-{{cartItem.variant_id}} class="item-price">{{ cartItem.selling_price | currency(currency) }}</div>
                    </div>
                  </div>
                  <div class="theme-cart-table-data theme-cart-qty">
                    <span class="theme-qty-head"><b>{{i18n.get("cart.quantity.label")}} : </b></span>
                    {% if page.template_vars.product_details_style == "01" %}
                      <span class="theme-quantity-field-button">
                        <input type="text" value="{{ cartItem.quantity }}" onfocus="showUpdate('{{cartItem.variant_id}}')" onkeypress='return productQuantity(event);' data-zs-quantity data-zs-product-variant-id="{{cartItem.variant_id}}">
                        <div class="theme-cart-quantity-error" data-quantity-error-cart="{{cartItem.variant_id}}" style="display:none">
                          {{i18n.get("product.message.error.invalid_quantity")}}
                        </div>
                        <button class="zpbutton zpbutton-type-primary zpbutton-size-sm theme-cart-update-button" data-theme-update="{{cartItem.variant_id}}" data-zs-update data-zs-product-variant-id="{{cartItem.variant_id}}">
                          {{i18n.get("cart.update.label")}}
                        </button>
                        <div class="zpbutton zpbutton-type-primary zpbutton-size-sm theme-cart-update-loading-button">
                          <span class="theme-cart-update-loading-dots">. . .</span>
                        </div>
                      </span>
                    {% endif %}
                    {% if page.template_vars.product_details_style == "02" %}
